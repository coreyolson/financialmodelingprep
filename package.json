{
  "name": "financialmodelingprep-node",
  "version": "1.0.6",
  "types": "./index.d.ts",
  "main": "./dist/financialmodelingprep.bundle.js",
  "module": "./dist/financialmodelingprep.bundle.js",
  "exports": {
    ".": "./dist/financialmodelingprep.bundle.js"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "nodemon --exec 'jest --watchAll'",
    "dev": "nodemon ./examples/__quickstart.mjs",
    "build": "rimraf dist && rollup -c",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coreyolson/financialmodelingprep-node.git"
  },
  "author": "Corey Olson",
  "contributors": [
    "Corey Olson (https://coreyolson.com/)"
  ],
  "keywords": [
    "financial",
    "api",
    "nodejs",
    "financial-modeling-prep",
    "finance",
    "stock-market",
    "financial-data",
    "investing",
    "market-data",
    "stocks",
    "economics",
    "financial-statements",
    "cryptocurrency",
    "forex",
    "etf",
    "mutual-fund",
    "market-analysis",
    "company-profiles",
    "economic-data",
    "financial-ratios"
  ],
  "license": "MIT",
  "description": "A Node.js client for the FinancialModelingPrep API",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-modules-commonjs": "^7.24.8",
    "@babel/preset-env": "^7.25.4",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "babel-jest": "^29.7.0",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "nodemon": "^3.1.4",
    "rimraf": "^6.0.1",
    "rollup": "^4.21.3"
  },
  "jest": {
    "verbose": false,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "html",
      "text"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "cjs": {
        "plugins": [
          "@babel/plugin-transform-modules-commonjs"
        ]
      }
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
