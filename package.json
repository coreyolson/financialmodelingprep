{
  "name": "financialmodelingprep-node",
  "version": "1.0.3",
  "type": "module",
  "types": "./index.d.ts",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "exports": {
    "require": "./dist/index.cjs.js",
    "import": "./dist/index.esm.js"
  },
  "scripts": {
    "dev": "nodemon ./examples/__quickstart.js",
    "test": "jest",
    "test:watch": "nodemon --exec 'jest --watchAll'",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "babel src --out-dir dist --extensions '.js' --out-file-extension '.esm.js'",
    "build:cjs": "babel src --out-dir dist --extensions '.js' --out-file-extension '.cjs.js' --plugins @babel/plugin-transform-modules-commonjs",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coreyolson/financialmodelingprep-node.git"
  },
  "author": "Corey Olson",
  "contributors": [
    "Corey Olson (https://coreyolson.com/)"
  ],
  "keywords": [
    "financial",
    "api",
    "nodejs",
    "financial-modeling-prep",
    "finance",
    "stock-market",
    "financial-data",
    "investing",
    "market-data",
    "stocks",
    "economics",
    "financial-statements",
    "cryptocurrency",
    "forex",
    "etf",
    "mutual-fund",
    "market-analysis",
    "company-profiles",
    "economic-data",
    "financial-ratios"
  ],
  "license": "MIT",
  "description": "A Node.js client for the FinancialModelingPrep API",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-modules-commonjs": "^7.24.8",
    "@babel/preset-env": "^7.25.4",
    "babel-jest": "^29.7.0",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "nodemon": "^3.1.4"
  },
  "jest": {
    "verbose": false,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "html",
      "text"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "cjs": {
        "plugins": [
          "@babel/plugin-transform-modules-commonjs"
        ]
      }
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
